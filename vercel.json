{
  "version": 2,
  "redirects": [
    {
      "source": "/ancien-lien",
      "destination": "/nouveau-lien",
      "permanent": true
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
